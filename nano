<?php
defined('TYPO3') or die();

use TYPO3\CMS\Extbase\Utility\ExtensionUtility;

(static function() {
    $extensionKey = 'anatolkin_restaurant';

    ExtensionUtility::registerPlugin($extensionKey, 'MenuList', 'Restaurant: Menu List');
    ExtensionUtility::registerPlugin($extensionKey, 'CategoryNav', 'Restaurant: Category Navigation');
    ExtensionUtility::registerPlugin($extensionKey, 'ItemDetails', 'Restaurant: Item Details');
    ExtensionUtility::registerPlugin($extensionKey, 'Checkout', 'Restaurant: Checkout');
    ExtensionUtility::registerPlugin($extensionKey, 'Locations', 'Restaurant: Locations');
    ExtensionUtility::registerPlugin($extensionKey, 'Integrations', 'Restaurant: Integrations');
})();
